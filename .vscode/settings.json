{
  // Configuration MCP Servers
  "mcp.servers": {
    "latex-core": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-latex"
      ],
      "disabled": false,
      "autoStart": true
    },
    "tikz-preview": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-tikz"
      ],
      "disabled": false,
      "autoStart": true
    },
    "bibtex-manager": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-bibtex"
      ],
      "disabled": false,
      "autoStart": true
    },
    "diagram-generator": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-diagrams"
      ],
      "disabled": false,
      "autoStart": true
    },
    "latex-validator": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-chktex"
      ],
      "disabled": false,
      "autoStart": true
    },
    "pdf-preview": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-pdf"
      ],
      "disabled": false,
      "autoStart": true
    },
    "latex-packages": {
      "command": "python3",
      "args": [
        "-m",
        "mcp_servers.latex_packages"
      ],
      "disabled": false,
      "autoStart": true
    },
    "latex-templates": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-latex-templates"
      ],
      "disabled": false,
      "autoStart": true
    }
  },
  // Configuration LaTeX Workshop
  "latex-workshop.latex.outDir": "./build",
  "latex-workshop.latex.autoBuild.run": "onSave",
  "latex-workshop.latex.autoClean.run": "onFailed",
  "latex-workshop.latex.recipe.default": "xelatex",
  // Recipes pour XeLaTeX
  "latex-workshop.latex.recipes": [
    {
      "name": "xelatex",
      "tools": [
        "xelatex",
        "biber",
        "xelatex",
        "xelatex"
      ]
    },
    {
      "name": "latexmk (xelatex)",
      "tools": [
        "latexmk"
      ]
    },
    {
      "name": "quick compile",
      "tools": [
        "xelatex"
      ]
    }
  ],
  // Tools configuration
  "latex-workshop.latex.tools": [
    {
      "name": "xelatex",
      "command": "xelatex",
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "-shell-escape",
        "-output-directory=%OUTDIR%",
        "%DOC%"
      ],
      "env": {
        "TEXMFHOME": "./.texmf",
        "TEXINPUTS": "./styles//:.///:",
        "BIBINPUTS": ".///:"
      }
    },
    {
      "name": "latexmk",
      "command": "latexmk",
      "args": [
        "-xelatex",
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "-shell-escape",
        "-outdir=%OUTDIR%",
        "%DOC%"
      ]
    },
    {
      "name": "biber",
      "command": "biber",
      "args": [
        "--output-directory=%OUTDIR%",
        "%DOCFILE%"
      ]
    }
  ],
  // PDF Viewer configuration
  "latex-workshop.view.pdf.viewer": "tab",
  "latex-workshop.view.pdf.zoom": "page-width",
  "latex-workshop.view.pdf.scrollMode": 1,
  "latex-workshop.view.pdf.spreadMode": 0,
  // IntelliSense
  "latex-workshop.intellisense.package.enabled": true,
  "latex-workshop.intellisense.citation.format": "bibtex",
  "latex-workshop.intellisense.label.format": "%c",
  // Linting
  "latex-workshop.linting.chktex.enabled": true,
  "latex-workshop.linting.chktex.exec.path": "chktex",
  "latex-workshop.linting.chktex.args": [
    "-wall",
    "-n22",
    "-n30",
    "-e16"
  ],
  // Build configuration
  "latex-workshop.latex.clean.fileTypes": [
    "*.aux",
    "*.bbl",
    "*.blg",
    "*.idx",
    "*.ind",
    "*.lof",
    "*.lot",
    "*.out",
    "*.toc",
    "*.acn",
    "*.acr",
    "*.alg",
    "*.glg",
    "*.glo",
    "*.gls",
    "*.ist",
    "*.fls",
    "*.log",
    "*.fdb_latexmk",
    "*.snm",
    "*.nav",
    "*.synctex.gz"
  ],
  // Compilation dans un dossier séparé
  "latex-workshop.latex.outputDir": "./build",
  // TikZ configuration
  "latex-workshop.latex.tikz.previewMode": "preview-in-tab",
  "latex-workshop.latex.tikz.command": "pdflatex",
  // Snippet configuration
  "latex-workshop.intellisense.snippet.enabled": true,
  "latex-workshop.intellisense.snippet.suggestions": true,
  // Workspace specific settings
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 1000,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit"
  },
  // File associations
  "files.associations": {
    "*.tikz": "latex",
    "*.pgf": "latex",
    "*.bib": "bibtex"
  },
  // Spell checking
  "ltex.enabled": true,
  "ltex.language": "fr",
  "ltex.latex.commands": {
    "\\lstset{}": "ignore",
    "\\lstdefinelanguage{}{}": "ignore",
    "\\titleformat{}[][]{}{}{}{}{}": "ignore",
    "\\titlespacing*{}{}{}{}": "ignore"
  },
  "ltex.latex.environments": {
    "lstlisting": "ignore",
    "tikzpicture": "ignore"
  },
